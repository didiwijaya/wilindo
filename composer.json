{
    "name": "didiwijaya/wilindo",
    "description": "Package Laravel (Migration & Seeder) untuk wilayah Indonesia berdasarkan data SPLP Kemendagri",
    "license": "MIT",
    "keywords": [
        "laravel",
        "php",
        "wilayah",
        "indonesia",
        "provinces",
        "cities",
        "districts",
        "villages"
    ],
    "authors": [
        {
            "name": "Didi Wijaya",
            "email": "ddwijaya3@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DidiWijaya\\WilIndo\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DidiWijaya\\WilIndo\\WilIndoServiceProvider"
            ],
            "aliases": {}
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
